{
  "name": "react-native-sonoma-analytics",
  "version": "0.0.1",
  "description": "React Native plugin for Sonoma Analytics",
  "main": "SonomaAnalytics.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/react-native-sonoma-private.git"
  },
  "keywords": [
    "react-native",
    "sonoma",
    "analytics"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/react-native-sonoma-private/issues"
  },
  "homepage": "https://github.com/microsoft/react-native-sonoma-private#readme",
  "dependencies": {
    "sonoma-react-native-link-scripts": "../RNSonomaLinkScript"
  },
  "rnpm": {
    "params": [
      {
        "type": "list",
        "name": "whenToEnableAnalytics",
        "message": "For the Android app, should user tracking be enabled automatically ?",
        "choices": [
          {
            "name": "Enable Automatically",
            "value": "ALWAYS_SEND"
          },
          {
            "name": "Enable in JavaScript",
            "value": "ENABLE_IN_JS"
          }
        ]
      }
    ],
    "android": {
      "packageInstance": "new RNSonomaAnalyticsPackage(MainApplication.this, ${whenToEnableAnalytics})"
    },
    "commands": {
      "prelink": "node node_modules/react-native-sonoma-analytics/scripts/prelink",
      "postlink": "node node_modules/react-native-sonoma-analytics/scripts/postlink"
    }
  }
}
